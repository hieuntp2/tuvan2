@model IEnumerable<tuvantuyensinhsv.v2.Controllers.SearchResultDeAnTuyenSinh>

@if (Model != null)
{
    if (Model.Count() > 0)
    {
        <table class="table">
            <tr>
                <th>
                   Trường
                </th>
                <th>
                    Ngành
                </th>
                <th>
                    Điểm 2014
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.tentruong
                    </td>
                    <td>
                        @item.tennganh
                    </td>
                    <td>
                       @item.diem
                    </td>                    
                </tr>
            }
        </table>

    }
    else
    {
        <p>Không có kết quả phù hợp</p>
    }
}

